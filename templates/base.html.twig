<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>{% block title %}Welcome!{% endblock %}</title>
		<link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
		{# {{ encore_entry_link_tags('app') }} #}
		<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
		<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css" integrity="sha512-9usAa10IRO0HhonpyAIVpjrylPvoDwiPUiKdWk5t3PyolY1cOd4DSE0Ga+ri4AuTroPR5aQvXU9xC6qOPnzFeg==" crossorigin="anonymous">
		<link rel="stylesheet" href="/css/style.css">
		{% endblock %}>
		{% block javascripts %}
		{# {{ encore_entry_script_tags('app') }} #}
		<script src="https://code.jquery.com/jquery-3.4.1.min.js" integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo=" crossorigin="anonymous"></script>
		<script src="/js/gsf.js"></script>
		<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
		{% endblock %}
	</head>
	<body>
		<header>
			<nav class="navbar navbar-dark bg-dark">
				<a class="nav-link" href="{{ path('app_home') }}">Accueil</a>
				<a class="nav-link" href="{{ path('index_formation') }}">Formations</a>
				<a class="nav-link" href="{{ path('index_session') }}">Sessions</a>
				<a class="nav-link" href="{{ path('index_stagiaire') }}">Stagiaires</a>
				<a class="nav-link" href="{{ path('index_formateur') }}">Formateurs</a>
				<a class="nav-link" href="{{ path('index_module') }}">Modules</a>
				<a class="nav-link" href="{{ path('index_categorie') }}">Catégories</a>
			</nav>
		</header>
		<div class="container">
		{% block body %}

		{% endblock %}
		</div>
	</body>
</html>
